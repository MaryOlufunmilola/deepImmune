{
    // Note all the string value starting with "e" shall be considered as python expressin and will be evaluated using eval in python e.g. var = "e[1,2]" will be converted to var = [1,2]
    "learning_rate": 1e-2,
    "weight_decay": 0,
    //weight decay aka. l2-regulatrizer on parameters
    "batch_size": 50,
    "num_epochs": 50,
    "dropout_rate": 0.3,
    "embedding_size": 16,
    "batch_by_type" : false,
    "save_summary_steps": 2,
    "aggregate": "min",
    "best_model_metric" : "Response_auc",
    "metrics": "e[['Response', 'auc', 0, 0]]",
     // Format if metrices ['name of. variable', 'metrics', 'index of real data', 'index of predicted data'. Support multiple metrics.
    "l1_regularizer" : 0,
    "l2_regularizer" : 0,
    "pipeline_optimization" : 0,
    "out_channels_list": [],
    "kernel_sizes": [5, 5],
    "strides": [1, 3],
    "FC_size_list": [32, 16],
    "decoder_FC_size_list": [],
    // decoder assumed no colovution layer and configurable internal layers
    "input_indices": "e range(50) + [57,58] +  [83, 85]",
    //input indces:  None (implies all of the input indices) or  any python expression 
    "survival_indices": "e[]",
    "continuous_phenotype_indices" : "e[]",
    "binary_phenotype_indices": "e[41]",
    "loss_excluded_from_training" : "e[]",
    "num_workers": 4

}