{
    // Note all the string value starting with "e" shall be considered as python expressin and will be evaluated using eval in python e.g. var = "e[1,2]" will be converted to var = [1,2]
    "learning_rate": 1e-3,
    "weight_decay": 0,
    //weight decay aka. l2-regulatrizer on parameters
    "batch_size": 50,
    "num_epochs": 500,
    "dropout_rate": 0.3,
    "embedding_size": 16,
    "batch_by_type" : false,
    "save_summary_steps": 2,
    "aggregate": "min",
    "best_model_metric" : "negative_loss",
    "metrics": "e[['SNV.Neoantigens', 'correlation', 0, 0],['Nonsilent.Mutation.Rate', 'correlation', 3, 3]]",
     // Format if metrices ['name of. variable', 'metrics', 'index of real data', 'index of predicted data'. Support multiple metrics.
    "l1_regularizer" : 0,
    "l2_regularizer" : 0,
    "pipeline_optimization" : 0,
    "out_channels_list": [],
    "kernel_sizes": [5, 5],
    "strides": [1, 3],
    "FC_size_list": [32, 16],
    "input_indices": "None",
    //input indces:  None (implies all of the input indices) or  any python expression 
    "survival_indices": "e[]",
    "continuous_phenotype_indices" : "e[42, 36, 36, 43, 36, 36, 36, 36]",
    "binary_phentoype_indices": "e[]",
    "loss_excluded_from_training" : "e[1,2,4,5,6,7]",
    "num_workers": 4,

}

